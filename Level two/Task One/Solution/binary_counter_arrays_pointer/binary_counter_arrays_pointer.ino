  {1, 1, 1, 1, 0},
  {1, 1, 1, 1, 1}
};
byte* p;
byte c = 0;
void setup()
{
  Serial.begin(9600);
  for (int i = 0; i <= 4; i++) {
    pinMode(led[i], OUTPUT);
  }
}


void loop()
{
  for (p = &val[0][0]; p <= &val[31][4]; p++)
  {
    digitalWrite(led[c % 5], *p);
    if (c % 5 == 4)delay(1000);
    c++;
  }
  delay(2000);
}
